class Student {
    String name;
    int age;

    void display() {
        System.out.println("Name: " + name + ", Age: " + age);
    }
}

public class Main1 {
    public static void main(String[] args) {
        Student s = new Student();
        s.name = "Charul Bhanarkar";
        s.age = 18;
        s.display();
    }
}
